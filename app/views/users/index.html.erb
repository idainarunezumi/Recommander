<div class="user-index">
  <div class="container">
    <h1 class="users-heading">ユーザー一覧</h1>
    <div class="users-wrapper">
     <% @users.each do |user| %>
    <div class="user-index-item">
      <div class="user-left-img">
        <img src="<%= "/public/#{user.image_name}" %>">
      </div>
      <%=link_to(user.name,"/users/#{user.id}") %>
    </div>
    <% end %>
   </div>
  </div>

</div>
